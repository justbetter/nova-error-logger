{
    "name": "justbetter/nova-error-logger",
    "description": "Laravel Nova integration for Laravel Error Logger.",
    "require": {
        "php": "^8.0",
        "justbetter/laravel-error-logger": "^2.2",
        "laravel/framework": "^8.0 || ^9.0",
        "laravel/nova": "^3.0",
        "marshmallow/nova-date-range-filter": "^1.0"
    },
    "authors": [
        {
            "name": "Ramon Rietdijk",
            "email": "ramon@justbetter.nl",
            "role": "Developer"
        },
        {
            "name": "Vincent Boon",
            "email": "vincent@justbetter.nl",
            "role": "Developer"
        }
    ],
    "type": "laravel-package",
    "autoload": {
        "psr-4": {
            "JustBetter\\NovaErrorLogger\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "JustBetter\\NovaErrorLogger\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
