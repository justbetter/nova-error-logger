{
  "name": "justbetter/nova-error-logger",
  "description": "Laravel Nova integration for Laravel Error Logger.",
  "require": {
    "php": "^8.0",
    "justbetter/laravel-error-logger": "^2.0",
    "laravel/framework": "*",
    "laravel/nova": "*"
  },
  "authors": [
    {
      "name": "Ramon Rietdijk",
      "email": "ramon@justbetter.nl",
      "role": "Developer"
    }
  ],
  "type": "laravel-package",
  "autoload": {
    "psr-4": {
      "JustBetter\\NovaErrorLogger\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "JustBetter\\LaravelNovaErrorLogger\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}